{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"jsx": "react",
		"outDir": "Assemblies",
		"rootDir": "Roblox",
		"noUnusedLocals": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"preserveSymlinks": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"stripInternal": true,
		"removeComments": true,
		"declaration": true,
		"declarationDir": "Lib"
	}
}
